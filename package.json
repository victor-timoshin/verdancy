		"bufferutil": "^4.0.3",
		"cors": "^2.8.5",
		"socket.io": "^4.0.1",
		"socket.io-client": "^4.0.1",
		"strict-event-emitter-types": "^2.0.0",
		"underscore": "^1.13.1",
		"utf-8-validate": "^5.0.4",
		"vue": "^3.0.11",
		"vue-class-component": "^8.0.0-rc.1",
		"vue-router": "^4.0.6",
		"ws": "^7.4.4"
	},
	"devDependencies": {
		"@types/node": "^14.14.41",
		"@types/underscore": "^1.11.1",
		"@types/webpack-env": "^1.16.0",
