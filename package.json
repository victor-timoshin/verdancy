	},
	"bugs": {
		"url": "https://github.com/victor-timoshin/verdancy/issues"
	},
	"keywords": [
		"webpack",
		"node",
		"typescript",
		"scss",
		"vue"
	],
	"scripts": {
		"clean": "node_modules/.bin/ts-node -P tsconfig.json ./bin/clean.ts",
		"start-server:ts": "node_modules/.bin/ts-node -P tsconfig.json -r tsconfig-paths/register ./bin/server/index.ts",
		"start-server": "node ./bin/server/index.js",
		"build-server:dev": "cross-env NODE_ENV=development AUTOPREFIXER=autoplace node_modules/.bin/ts-node -P tsconfig.json ./bin/bundle.server.ts",
		"build-server": "cross-env NODE_ENV=production AUTOPREFIXER=autoplace node_modules/.bin/ts-node -P tsconfig.json ./bin/bundle.server.ts",
		"build-client:dev": "cross-env NODE_ENV=development AUTOPREFIXER=autoplace node_modules/.bin/ts-node -P tsconfig.json ./bin/bundle.ts",
		"build-client": "cross-env NODE_ENV=production AUTOPREFIXER=autoplace node_modules/.bin/ts-node -P tsconfig.json ./bin/bundle.ts",
		"build:dev": "npm run clean && npm run build-client:dev && npm run build-server:dev",
		"build": "npm run clean && npm run build-client && npm run build-server",
		"watch": "concurrently \"node_modules/.bin/ts-node -P tsconfig.json ./bin/server/hotreload.ts\" \"nodemon ./bin/server/index.ts --watch dist -e js\" --kill-others",
		"tests": "jest"
	},
	"dependencies": {
		"@timcowebapps/common.ooscss": "^0.3.18",
		"@timcowebapps/common.smascss.styles": "^0.23.80",
		"@timcowebapps/common.system": "^0.4.13",
		"@timcowebapps/common.utils": "^0.16.68",
