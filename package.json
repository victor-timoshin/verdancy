		"@vue/compiler-sfc": "^3.0.11",
		"autoprefixer": "^10.2.5",
		"compression-webpack-plugin": "^7.1.2",
		"concurrently": "^6.0.2",
		"cross-env": "^7.0.3",
		"css-loader": "^5.2.2",
		"html-webpack-plugin": "^5.3.1",
		"mini-css-extract-plugin": "^1.4.1",
		"nodemon": "^2.0.7",
		"postcss-loader": "^5.2.0",
		"sass": "^1.32.10",
		"sass-loader": "^11.0.1",
		"style-loader": "^2.0.0",
		"ts-loader": "^8.1.0",
